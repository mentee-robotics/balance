        },
        {
            "name": "run with default yaml",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--yaml",
                "/home/administrator/code/ros2_workspace/balance/configs/default.yaml"
            ],
